  "scripts": {
    "start": "npm run server",
    "prod": "npm run lint && npm run build && npm run server:prod",
    "server": "node --experimental-specifier-resolution=node --loader ts-node/esm ./server/express.ts",
    "server:embed": "parcel --port 8233 --no-hmr embed/embed.ts",
    "server:prod": "cross-env NODE_ENV=production ts-node ./server/index.ts",
    "lint": "eslint",
    "pf": "run-p -c typecheck lint",
    "typecheck": "tsc --noemit",
    "build:server": "vite build",
    "build:embed": "parcel build --dist-dir dist/client/assets/static embed/embed.ts",
    "build:lambda": "yarn run esbuild --platform=node --outdir=dist --external:'@aws-sdk/*' --bundle server/*lambda.ts",
    "build:all": "run-p build:server build:lambda build:embed"
  },
